{% if subscriptions is not empty %}
    {% if not isMaxNumber %}
        <i class='material-icons subscribe blue-text' data-date='{{ date }}'>control_point</i>
    {% endif %}
    <i class='material-icons unsubscribe red-text' data-date='{{ date }}'>highlight_off</i>
{% else %}
    <i class='material-icons subscribe blue-text' data-date='{{ date }}'>control_point</i>
{% endif %}
